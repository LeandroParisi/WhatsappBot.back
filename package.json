    "start": "ts-node src/index.ts",
    "post-deploy": "",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/index.ts",
    "migrate": "node-pg-migrate",
    "seed": "sh ./src.v2/Infrastructure/StaticFiles/Seeds/_run_seeds.sh",
    "unseed": "npx sequelize-cli db:seed:undo:all --debug",
    "unmigrate": "npx sequelize-cli db:migrate:undo:all --debug",
    "db:up": "npx sequelize-cli db:create --debug",
    "db:down": "npx sequelize-cli db:drop --debug",
    "up": "npm run migrate && npm run seed",
    "down": "npm run unseed && npm run unmigrate",
    "createRoute": "sh ./templates/2.new_route.sh"
  },
