{
  "name": "backEnd_whatsappBot",
  "version": "1.0.0",
  "description": "Back end for whatsapp Bot",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/index.js",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo:all",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo:all",
    "db": "npx sequelize-cli db:create",
    "up": "npm run migrate && npm run seed",
    "down": "npm run unseed && npm run unmigrate"
  },
